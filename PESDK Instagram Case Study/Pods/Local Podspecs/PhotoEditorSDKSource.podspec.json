{
  "name": "PhotoEditorSDKSource",
  "version": "8.1.0",
  "license": {
    "type": "Commercial",
    "file": "LICENSE.md"
  },
  "summary": "Integrate the photo editor into your own HTML5, iOS or Android app - in minutes!",
  "homepage": "https://www.photoeditorsdk.com",
  "social_media_url": "https://twitter.com/photoeditorsdk",
  "authors": {
    "9elements GmbH": "contact@9elements.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/imgly/pesdk-ios.git",
    "tag": "8.1.0"
  },
  "source_files": [
    "PhotoEditorSDK/**/*.{h,m,swift}"
  ],
  "resources": [
    "Resources/Assets/*",
    "Resources/en.lproj/*",
    "Resources/Filter Responses/*.png",
    "Resources/Fonts/*",
    "Resources/Kernels/*",
    "Resources/Shaders/*"
  ],
  "pod_target_xcconfig": {
    "COMPRESS_PNG_FILES": 0
  },
  "frameworks": [
    "Accelerate",
    "AVFoundation",
    "CFNetwork",
    "CoreGraphics",
    "CoreImage",
    "CoreLocation",
    "CoreMotion",
    "CoreText",
    "Foundation",
    "GLKit",
    "ImageIO",
    "MobileCoreServices",
    "OpenGLES",
    "Photos",
    "UIKit"
  ],
  "requires_arc": true
}
